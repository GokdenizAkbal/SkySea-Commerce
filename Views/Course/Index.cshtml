@model List<Course>

<div class="d-flex justify-content-between align-items-center">
    <h1 class="h3 b-3">Courses</h1>
    <a class="btn btn-outline-danger" asp-controller="Course" asp-action="List">All Courses</a>
</div>
<div class="row">

    @foreach (Course course in Model)
    {
        @if (course.IsHome)
        {
            <div class="col-md-4">
                @await Html.PartialAsync("_CourseCard", course)
            </div>
        }
    }



</div>